[run]
branch = True
source = src

[report]
exclude_lines =
    pragma: no cover
    except ImportError:
    \# Python <
    raise NotImplementedError
    raise TypeError\(f?"Unexpected
    assert False,
    \s+next\($
    if MYPY:
    if TYPE_CHECKING:
    ^\s+\.\.\.$
    ^\s+pass$
    @pytest.mark.skip

ignore_errors = True
